        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"StrongP@ss123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/api/login",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "api",
                "login"
              ]
            },
            "description": "Login with email and password. Returns user data and access token."
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Auto-save token to both collection and environment variables",
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    if (jsonData.token) {",
                  "        // Save to collection variable (works without environment)",
                  "        pm.collectionVariables.set(\"authToken\", jsonData.token);",
                  "        ",
                  "        // Save to environment variable (if environment is active)",
                  "        pm.environment.set(\"authToken\", jsonData.token);",
                  "        ",
                  "        console.log(\"Token saved to collection and environment:\", jsonData.token);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
